    "docker:run": "docker run -p 7040:7040 --env-file .env nexpo-api-scala",
    "docker:compose": "docker-compose up -d",
    "clean": "sbt clean"
  },
  "keywords": [
    "nexpo",
    "api",
    "scala",
    "play-framework",
    "auth0",
    "mindsdb"
  ],
  "author": "Nexpo Team",
  "license": "MIT",
  "engines": {
    "java": ">=17"
  }
}
