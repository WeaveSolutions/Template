{
  "name": "nexpo-template",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.20.0",
  "scripts": {
    "start": "pnpm run dev",
    "dev": "node workspace-config.js && pnpx concurrently -n \"PLATFORMS,API,DB\" -c \"cyan,yellow,green\" \"node scripts/dev-enabled-platforms.js\" \"pnpm dev:backends\" \"pnpm start:mindsdb\"",
    "setup": "pnpm run dev",
    "next": "cd apps/nexpo/nextWeb && pnpm dev",
    "expo": "cd apps/nexpo/expoMobile && npx expo start",
    "tauri": "cd apps/desktop && pnpm run desktop",
    "svelte": "cd apps/taurte/svelteWeb && pnpm dev",
    "dev:backends": "pnpx concurrently -n \"API,AUTH\" -c \"blue,magenta\" \"pnpm dev:api\" \"pnpm dev:auth\"",
    "dev:api": "cd microservices/api/api-typescript && pnpm dev",
    "dev:auth": "cd microservices/api/api-typescript && pnpm dev:auth",
    "setup:backend": "cd microservices/api/api-typescript && node setup.js && pnpm install",
    "setup:auth": "cd microservices/api/api-typescript && node setup.js && pnpm install",
    "build": "node scripts/build-enabled-platforms.js",
    "build:next": "cd apps/nexpo/nextWeb && pnpm build",
    "build:expo": "cd apps/nexpo/expoMobile && npx expo build",
    "build:tauri": "cd apps/desktop && pnpm run build",
    "build:svelte": "cd apps/taurte/svelteWeb && pnpm build",
    "build:api": "cd microservices/api/api-typescript && pnpm build",
    "reset": "pnpm clean && pnpm install",
    "clean": "rm -rf node_modules packages/*/node_modules apps/*/node_modules microservices/*/node_modules",
    "lint": "eslint .",
    "test": "pnpm -r test",
    "dev:development": "cross-env NODE_ENV=development pnpm run dev",
    "dev:staging": "cross-env NODE_ENV=staging pnpm run dev",
    "dev:production": "cross-env NODE_ENV=production pnpm run dev",
    "terraform:init": "node scripts/generate-terraform.js init",
    "terraform:plan": "node scripts/generate-terraform.js plan",
    "terraform:apply": "node scripts/generate-terraform.js apply",
    "terraform:generate": "node scripts/generate-terraform.js generate",
    "workspace:config": "node workspace-config.js",
    "workspace:nexpo": "node workspace-config.js --platform=nexpo",
    "workspace:taurte": "node workspace-config.js --platform=taurte",
    "workspace:all": "node workspace-config.js --platform=all",
    "start:mindsdb": "cross-env DEBUG= LOG_LEVEL= node scripts/start-mindsdb.js start",
    "stop:mindsdb": "node scripts/start-mindsdb.js stop",
    "restart:mindsdb": "node scripts/start-mindsdb.js restart",
    "logs:mindsdb": "node scripts/start-mindsdb.js logs",
    "workflows:generate": "node scripts/generate-workflows.js generate",
    "workflows:list": "node scripts/generate-workflows.js list",
    "workflows:remove": "node scripts/generate-workflows.js remove",
    "check:flags": "node scripts/check-dev-flags.js",
    "kill:ports": "powershell -ExecutionPolicy Bypass -File scripts/kill-dev-ports.ps1",
    "clean:start": "pnpm kill:ports && pnpm dev"
  },
  "workspaces": [
    "apps/desktop",
    "packages/shared-nexpo/shared-components",
    "packages/shared-nexpo/shared-db",
    "packages/shared-nexpo/shared-fonts",
    "packages/shared-nexpo/shared-hooks",
    "packages/shared-nexpo/shared-pages",
    "packages/shared-nexpo/shared-provider",
    "packages/shared-nexpo/shared-ui",
    "packages/shared-nexpo/shared-utils",
    "apps/nexpo/nextWeb",
    "apps/nexpo/expoMobile",
    "apps/taurte/svelteWeb",
    "apps/taurte/tauriMobile",
    "packages/shared-taurte",
    "microservices/api/api-typescript"
  ],
  "resolutions": {
    "@types/react": "18.3.17",
    "@types/react-dom": "18.3.5"
  },
  "overrides": {
    "@types/react": "18.3.17",
    "@types/react-dom": "18.3.5"
  },
  "pnpm": {
    "overrides": {
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "vite": "5.4.21",
      "@react-native-async-storage/async-storage": "1.24.0"
    }
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-native": "^12.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/bonjour": "^3.5.13",
    "@types/connect": "^3.4.38",
    "@types/connect-history-api-fallback": "^1.5.4",
    "@types/estree": "^1.0.5",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.41",
    "@types/glob": "^8.1.0",
    "@types/html-minifier-terser": "^6.0.0",
    "@types/http-errors": "^2.0.4",
    "@types/http-proxy": "^1.17.14",
    "@types/istanbul-lib-coverage": "^2.0.6",
    "@types/istanbul-lib-report": "^3.0.3",
    "@types/istanbul-reports": "^3.0.4",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.10.2",
    "@types/prop-types": "^15.7.11",
    "@types/qs": "^6.9.10",
    "@types/range-parser": "^1.2.7",
    "@types/react": "18.3.17",
    "@types/react-dom": "18.3.5",
    "@types/retry": "^0.12.5",
    "@types/scheduler": "^0.16.8",
    "@types/send": "^0.17.4",
    "@types/serve-static": "^1.15.5",
    "@types/sockjs": "^0.3.36",
    "@types/stack-utils": "^2.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.10",
    "@types/yargs": "^17.0.32",
    "@types/yargs-parser": "^21.0.3",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "js-yaml": "^4.1.0",
    "nativewind": "^4.1.23",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "turbo": "^1.10.12",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^2.3.0",
    "@expo/next-adapter": "^6.0.0",
    "@prisma/client": "^6.8.2",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-navigation/native": "^7.1.9",
    "@stripe/react-stripe-js": "^3.6.0",
    "@stripe/stripe-js": "^7.2.0",
    "@supabase/supabase-js": "^2.49.4",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo-auth-session": "^6.1.5",
    "expo-crypto": "^14.1.4",
    "expo-web-browser": "^14.1.6",
    "firebase": "^11.8.1",
    "i18next": "^25.0.1",
    "i18next-browser-languagedetector": "^8.0.5",
    "lucide-react": "^0.503.0",
    "next-seo": "^6.6.0",
    "next-sitemap": "^4.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^15.4.1",
    "react-native": "0.73.2",
    "react-native-reanimated": "^3.17.3",
    "react-native-safe-area-context": "^5.4.1",
    "react-native-screens": "^4.10.0",
    "solito": "4.4.1",
    "stripe": "^18.0.0",
    "tailwind-merge": "^3.2.0",
    "uuid": "^11.1.0"
  }
}