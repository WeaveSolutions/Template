{
  "name": "nexpo-template",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.12.1",
  "scripts": {
    "start": "turbo run start",
    "dev": "pnpx concurrently \"pnpm dev:frontend\" \"pnpm dev:backend\"",
    "dev:frontend": "pnpx turbo run dev --filter=@nexpo/web",
    "dev:mobile": "pnpx turbo run start --filter=@nexpo/mobile",
    "dev:backend": "cd microservices/auth/server && pnpm dev",
    "dev:backend-only": "cd microservices/auth/server && pnpm dev",
    "setup:backend": "cd microservices/auth/server && node setup.js && pnpm install",
    "setup:auth": "cd microservices/auth/server && node setup.js && pnpm install",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules",
    "reset": "pnpm clean && pnpm install",
    "start:mindsdb": "node scripts/start-mindsdb.js start",
    "stop:mindsdb": "node scripts/start-mindsdb.js stop",
    "restart:mindsdb": "node scripts/start-mindsdb.js restart",
    "logs:mindsdb": "node scripts/start-mindsdb.js logs",
    "dev:full": "pnpx concurrently \"pnpm start:mindsdb\" \"pnpm dev\"",
    "dev:microservices": "pnpx concurrently \"pnpm dev:frontend\" \"pnpm dev:backend\" \"pnpm start:mindsdb\""
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "microservices/auth/server"
  ],
  "resolutions": {
    "@types/react": "18.3.17",
    "@types/react-dom": "18.3.5"
  },
  "overrides": {
    "@types/react": "18.3.17",
    "@types/react-dom": "18.3.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-native": "^12.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/bonjour": "^3.5.13",
    "@types/connect": "^3.4.38",
    "@types/connect-history-api-fallback": "^1.5.4",
    "@types/estree": "^1.0.5",
    "@types/express": "^4.17.21",
    "@types/express-serve-static-core": "^4.17.41",
    "@types/glob": "^8.1.0",
    "@types/html-minifier-terser": "^6.0.0",
    "@types/http-errors": "^2.0.4",
    "@types/http-proxy": "^1.17.14",
    "@types/istanbul-lib-coverage": "^2.0.6",
    "@types/istanbul-lib-report": "^3.0.3",
    "@types/istanbul-reports": "^3.0.4",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.5.0",
    "@types/prop-types": "^15.7.11",
    "@types/qs": "^6.9.10",
    "@types/range-parser": "^1.2.7",
    "@types/react": "18.3.17",
    "@types/react-dom": "18.3.5",
    "@types/react-native": "^0.73.0",
    "@types/retry": "^0.12.5",
    "@types/scheduler": "^0.16.8",
