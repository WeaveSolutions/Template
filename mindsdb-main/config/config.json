{
  "api": {
    "http": {
      "host": "0.0.0.0",
      "port": 47334
    },
    "mysql": {
      "host": "0.0.0.0", 
      "port": 47335,
      "database": "mindsdb",
      "ssl": false
    },
    "mongodb": {
      "host": "0.0.0.0",
      "port": 47336,
      "database": "mindsdb"
    }
  },
  "storage": {
    "data": "/data",
    "tmp": "/tmp"
  },
  "integrations": {
    "default_handler": "postgres",
    "handlers": {
      "postgres": {
        "class": "PostgresHandler", 
        "import": {
          "module": "mindsdb.integrations.handlers.postgres_handler"
        }
      },
      "mysql": {
        "class": "MySQLHandler",
        "import": {
          "module": "mindsdb.integrations.handlers.mysql_handler"
        }
      },
      "mongodb": {
        "class": "MongoDBHandler",
        "import": {
          "module": "mindsdb.integrations.handlers.mongodb_handler"
        }
      },
      "redis": {
        "class": "RedisHandler",
        "import": {
          "module": "mindsdb.integrations.handlers.redis_handler"
        }
      },
      "slack": {
        "class": "SlackHandler",
        "import": {
          "module": "mindsdb.integrations.handlers.slack_handler"
        }
      },
      "discord": {
        "class": "DiscordHandler",
        "import": {
          "module": "mindsdb.integrations.handlers.discord_handler"
        }
      },
      "github": {
        "class": "GitHubHandler",
        "import": {
          "module": "mindsdb.integrations.handlers.github_handler"
        }
      }
    }
  },
  "cloud": {
    "aws": {
      "enabled": false,
      "region": "us-east-1"
    },
    "gcp": {
      "enabled": false,
      "project_id": null
    },
    "azure": {
      "enabled": false,
      "tenant_id": null
    },
    "oci": {
      "enabled": false,
      "region": null
    }
  },
  "auth": {
    "http_auth_enabled": false,
    "username": null,
    "password": null
  },
  "debug": false,
  "log": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  },
  "cache": {
    "type": "local",
    "ttl": 300
  }
}
