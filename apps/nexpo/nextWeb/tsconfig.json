{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "target": "es2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@nexpo/shared-hooks": ["../../../packages/shared-nexpo/shared-hooks/src/index.ts"],
      "@nexpo/shared-hooks/*": ["../../../packages/shared-nexpo/shared-hooks/src/*"],
      "@nexpo/shared-provider": ["../../../packages/shared-nexpo/shared-provider/src/index.tsx"],
      "@nexpo/shared-provider/*": ["../../../packages/shared-nexpo/shared-provider/src/*"],
      "@nexpo/shared-utils": ["../../../packages/shared-nexpo/shared-utils/src/index.ts"],
      "@nexpo/shared-utils/*": ["../../../packages/shared-nexpo/shared-utils/src/*"],
      "@nexpo/shared-components": ["../../../packages/shared-nexpo/shared-components/src/index.ts"],
      "@nexpo/shared-components/*": ["../../../packages/shared-nexpo/shared-components/src/*"],
      "@nexpo/shared-pages": ["../../../packages/shared-nexpo/shared-pages/src/index.ts"],
      "@nexpo/shared-pages/*": ["../../../packages/shared-nexpo/shared-pages/src/*"],
      "@nexpo/shared-ui": ["../../../packages/shared-nexpo/shared-ui/src/index.ts"],
      "@nexpo/shared-ui/*": ["../../../packages/shared-nexpo/shared-ui/src/*"],
      "@shared/components": ["../../../packages/shared-nexpo/shared-components/src/index.ts"],
      "@shared/components/*": ["../../../packages/shared-nexpo/shared-components/src/*"],
      "@shared/pages": ["../../../packages/shared-nexpo/shared-pages/src/index.ts"],
      "@shared/pages/*": ["../../../packages/shared-nexpo/shared-pages/src/*"],
      "@shared/ui": ["../../../packages/shared-nexpo/shared-ui/src/index.ts"],
      "@shared/ui/*": ["../../../packages/shared-nexpo/shared-ui/src/*"],
      "@shared/utils": ["../../../packages/shared-nexpo/shared-utils/src/index.ts"],
      "@shared/utils/*": ["../../../packages/shared-nexpo/shared-utils/src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
